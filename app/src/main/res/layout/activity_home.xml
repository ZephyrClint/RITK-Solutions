<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5EDE55"
    tools:context=".home">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="423dp"
        android:layout_height="600dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toTopOf="@+id/userProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/path_58" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="414dp"
        android:layout_height="885dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/path_8" />

    <Button
        android:id="@+id/menuRsh"
        android:layout_width="198dp"
        android:layout_height="78dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:backgroundTint="#00CCFF"
        android:text="Menu"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/addFeedback"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.697" />

    <Button
        android:id="@+id/userProfile"
        android:layout_width="198dp"
        android:layout_height="78dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="156dp"
        android:backgroundTint="#00CCFF"
        android:onClick="userProfile"
        android:text="User Profile"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addFeedback"
        app:layout_constraintVertical_bias="0.191" />

    <Button
        android:id="@+id/addFeedback"
        android:layout_width="198dp"
        android:layout_height="78dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="81dp"
        android:backgroundTint="#00CCFF"
        android:onClick="addfeedback"
        android:text="Add Feedback"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/userProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menuRsh" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/sign_Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#C44141"
        android:backgroundTint="#580AFF"
        android:clickable="true"
        android:focusable="true"
        android:onClick="sign_out"
        app:backgroundTint="#3F51B5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userProfile"
        app:layout_constraintVertical_bias="0.607"
        app:srcCompat="@drawable/ic_exit"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>